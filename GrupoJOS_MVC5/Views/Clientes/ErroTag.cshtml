@{
    ViewBag.Title = "Erro ao Editar Tag";
}
@model GrupoJOS_MVC5.Models.Model_Tag

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>@ViewBag.Title</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home")">Home</a>
            </li>
            <li>
                <a href="@Url.Action("Tag","Clientes")">Tag</a>
            </li>
            <li class="active">
                <strong>@ViewBag.Title</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content">
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">

                    <div class="panel panel-warning">
                        <div class="panel-heading text-center">
                            <h2>Erro ao editar Tag</h2>
                        </div>
                        <div class="text-center">
                            <div class="inmodal">
                                <i class="fa fa-times modal-icon"></i>
                            </div>
                            <div class="panel-body">
                                <p>
                                    Não foi possível editar a Tag.<br>
                                    O Cliente <u>@Model.cliente.Nome</u> possui um agendamento para o usuário <u>@Model.usuario.Nome</u>. <br>
                                    <br>
                                    <font color="red"><b>Remova</b></font> esse agendamento antes de realizar o processo de edição de Tag novamente.<br>
                                    <br>
                                    Para visualizar o Agendamento clique <a href="/AgendaCliente/Visualizar/@Model.idagenda">aqui</a>
                                </p>
                            </div>
                            <div class="panel-footer">
                                <a href="/Clientes/Tag"><button type="button" class="btn btn-w-m btn-default" data-toggle="modal" data-target="#myModal2"><i class="fa fa-arrow-left"></i> VOLTAR</button></a>
                            </div>
                        </div>
                    </div>



                </div>
            </div>
        </div>
    </div>
</div>
